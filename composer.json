{
    "name": "grikdotnet/generics",
    "description": "generics implementation in PHP",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-tokenizer": "*",
        "ext-ctype": "*",
        "ext-mbstring":"*",
        "ext-zend-opcache": "*",
        "composer-runtime-api": "*",
        "nikic/php-parser": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "grikdotnet\\generics\\": "generics/",
            "Generics\\": "generics/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Grigori Kochanov"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^12",
        "guzzlehttp/guzzle": "^7.9"
    }
}
